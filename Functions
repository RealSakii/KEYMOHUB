local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local humanoid
local infiniteJump = false
local holdingSpace = false

local NORMAL_SPEED = 20

local function setupCharacter(char)
	humanoid = char:WaitForChild("Humanoid")
	humanoid.WalkSpeed = NORMAL_SPEED
end

setupCharacter(player.Character or player.CharacterAdded:Wait())
player.CharacterAdded:Connect(setupCharacter)

-- à¸•à¸£à¸§à¸ˆà¸›à¸¸à¹ˆà¸¡à¸à¸”
UserInputService.InputBegan:Connect(function(input, gp)
	if gp then return end

	-- à¹€à¸›à¸´à¸”/à¸›à¸´à¸” Infinite Jump
	if input.KeyCode == Enum.KeyCode.B then
		infiniteJump = not infiniteJump
	end

	if input.KeyCode == Enum.KeyCode.Space then
		holdingSpace = true
	end
end)

UserInputService.InputEnded:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.Space then
		holdingSpace = false
	end
end)

-- à¸à¸£à¸°à¹‚à¸”à¸”à¸„à¹‰à¸²à¸‡
RunService.RenderStepped:Connect(function()
	if infiniteJump and holdingSpace and humanoid then
		humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
	end
end)

--Speed
-- SERVICES
local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local Mouse = Player:GetMouse()

-- VARIABLES
local Humanoid
local enabled = false
_G.WS = 150

-- CHARACTER LOAD / RESPAWN
local function SetupCharacter(char)
    Humanoid = char:WaitForChild("Humanoid")

    -- ANTI RESET SPEED
    Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
        if enabled and Humanoid.WalkSpeed ~= _G.WS then
            Humanoid.WalkSpeed = _G.WS
        end
    end)
end

if Player.Character then
    SetupCharacter(Player.Character)
end
Player.CharacterAdded:Connect(SetupCharacter)

-- KEY TOGGLE (X)
Mouse.KeyDown:Connect(function(key)
    if key:lower() == "x" then
        enabled = not enabled
        if enabled then
            Humanoid.WalkSpeed = _G.WS
        else
            Humanoid.WalkSpeed = 20
        end
    end
end)

-- ================= GUI =================
local gui = Instance.new("ScreenGui")
gui.Name = "SpeedGUI"
gui.Parent = game.CoreGui
gui.Enabled = false

local main = Instance.new("Frame", gui)
main.Size = UDim2.new(0,260,0,140)
main.Position = UDim2.new(0.02,0,0.5,-70)
main.BackgroundColor3 = Color3.fromRGB(25,25,25)
main.BorderSizePixel = 0
main.Active = true
main.Draggable = false
main.BackgroundTransparency = 0

local corner = Instance.new("UICorner", main)
corner.CornerRadius = UDim.new(0,12)

local title = Instance.new("TextLabel", main)
title.Size = UDim2.new(1,0,0,35)
title.Text = "à¸›à¸£à¸±à¸šà¸„à¸§à¸²à¸¡à¹€à¸£à¹‡à¸§à¹ƒà¸™à¸à¸²à¸£à¸§à¸´à¹ˆà¸‡"
title.TextColor3 = Color3.fromRGB(255,255,255)
title.BackgroundTransparency = 1
title.Font = Enum.Font.GothamBold
title.TextSize = 16

-- DRAG BAR (à¸¥à¸²à¸ GUI à¹„à¸”à¹‰à¹€à¸‰à¸žà¸²à¸°à¸•à¸£à¸‡à¸™à¸µà¹‰)
local dragBar = Instance.new("Frame", main)
dragBar.Size = UDim2.new(1,0,0,35)
dragBar.BackgroundTransparency = 1
dragBar.Active = true
dragBar.Selectable = true

local valueLabel = Instance.new("TextLabel", main)
valueLabel.Position = UDim2.new(0,0,0,40)
valueLabel.Size = UDim2.new(1,0,0,25)
valueLabel.Text = "à¸„à¸§à¸²à¸¡à¹€à¸£à¹‡à¸§ : 150"
valueLabel.TextColor3 = Color3.fromRGB(200,200,200)
valueLabel.BackgroundTransparency = 1
valueLabel.Font = Enum.Font.GothamBold
valueLabel.TextSize = 14

-- SLIDER BAR
local bar = Instance.new("Frame", main)
bar.Position = UDim2.new(0,20,0,75)
bar.Size = UDim2.new(1,-40,0,8)
bar.BackgroundColor3 = Color3.fromRGB(60,60,60)
bar.BorderSizePixel = 0

local barCorner = Instance.new("UICorner", bar)
barCorner.CornerRadius = UDim.new(1,0)

local fill = Instance.new("Frame", bar)
fill.Size = UDim2.new(0,0,1,0)
fill.BackgroundColor3 = Color3.fromRGB(0,170,255)
fill.BorderSizePixel = 0

local fillCorner = Instance.new("UICorner", fill)
fillCorner.CornerRadius = UDim.new(1,0)

-- SLIDER BUTTON
local knob = Instance.new("Frame", bar)
knob.Size = UDim2.new(0,16,0,16)
knob.Position = UDim2.new(0,-8,0.5,-8)
knob.BackgroundColor3 = Color3.fromRGB(255,255,255)
knob.BorderSizePixel = 0

local knobCorner = Instance.new("UICorner", knob)
knobCorner.CornerRadius = UDim.new(1,0)

-- SLIDER LOGIC
local UserInputService = game:GetService("UserInputService")
local dragging = false
local minSpeed, maxSpeed = 10, 300

local function UpdateSlider(x)
    local size = math.clamp((x - bar.AbsolutePosition.X) / bar.AbsoluteSize.X, 0, 1)
    fill.Size = UDim2.new(size,0,1,0)
    knob.Position = UDim2.new(size,-8,0.5,-8)

    local speed = math.floor(minSpeed + (maxSpeed - minSpeed) * size)
    _G.WS = speed
    valueLabel.Text = "à¸„à¸§à¸²à¸¡à¹€à¸£à¹‡à¸§ : "..speed

    if enabled and Humanoid then
        Humanoid.WalkSpeed = _G.WS
    end
end

knob.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = false
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        UpdateSlider(input.Position.X)
    end
end)

local UIS = game:GetService("UserInputService")

local dragging, dragStart, startPos

dragBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = main.Position
    end
end)

UIS.InputChanged:Connect(function(input)
    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        local delta = input.Position - dragStart
        main.Position = UDim2.new(
            startPos.X.Scale,
            startPos.X.Offset + delta.X,
            startPos.Y.Scale,
            startPos.Y.Offset + delta.Y
        )
    end
end)

UIS.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = false
    end
end)

-- TOGGLE GUI (X)
local UIS = game:GetService("UserInputService")
local guiVisible = false

UIS.InputBegan:Connect(function(input, gpe)
    if gpe then return end
    if input.KeyCode == Enum.KeyCode.RightControl then
        guiVisible = not guiVisible
        gui.Enabled = guiVisible
    end
end)



--teleport

local UserInputService = game:GetService("UserInputService")
local Players = game:GetService("Players")

local player = Players.LocalPlayer
local mouse = player:GetMouse()

-- à¸•à¸±à¹‰à¸‡à¸„à¹ˆà¸²à¸„à¸§à¸²à¸¡à¹„à¸à¸¥à¸ªà¸¹à¸‡à¸ªà¸¸à¸”à¸—à¸µà¹ˆà¸§à¸²à¸£à¹Œà¸›à¹„à¸”à¹‰ (à¸–à¹‰à¸²à¸•à¹‰à¸­à¸‡à¸à¸²à¸£à¸ˆà¸³à¸à¸±à¸”à¸£à¸°à¸¢à¸°)
local MAX_DISTANCE = 1000 

UserInputService.InputBegan:Connect(function(input, gameProcessed)
	-- à¸•à¸£à¸§à¸ˆà¸ªà¸­à¸šà¸§à¹ˆà¸²à¹„à¸¡à¹ˆà¹„à¸”à¹‰à¸à¸³à¸¥à¸±à¸‡à¸žà¸´à¸¡à¸žà¹Œà¹ƒà¸™à¸Šà¹ˆà¸­à¸‡ Chat à¹à¸¥à¸°à¸à¸”à¸›à¸¸à¹ˆà¸¡ 
	if not gameProcessed and input.KeyCode == Enum.KeyCode.T then
		local character = player.Character
		if character and character:FindFirstChild("HumanoidRootPart") then

			-- à¸•à¸£à¸§à¸ˆà¸ªà¸­à¸šà¸§à¹ˆà¸²à¹€à¸¡à¸²à¸ªà¹Œà¸Šà¸µà¹‰à¹„à¸›à¸—à¸µà¹ˆà¸•à¸³à¹à¸«à¸™à¹ˆà¸‡à¹ƒà¸”à¹ƒà¸™à¹‚à¸¥à¸ (Vector3)
			local targetPosition = mouse.Hit.Position

			-- à¸¢à¹‰à¸²à¸¢à¸•à¸³à¹à¸«à¸™à¹ˆà¸‡à¸•à¸±à¸§à¸¥à¸°à¸„à¸£ (à¸šà¸§à¸à¸„à¸§à¸²à¸¡à¸ªà¸¹à¸‡à¸‚à¸¶à¹‰à¸™ 3 à¸«à¸™à¹ˆà¸§à¸¢à¹€à¸žà¸·à¹ˆà¸­à¹„à¸¡à¹ˆà¹ƒà¸«à¹‰à¸ˆà¸¡à¸”à¸´à¸™)
			character:SetPrimaryPartCFrame(CFrame.new(targetPosition + Vector3.new(0, 3, 0)))
		end
	end

end)

--// Smart Smooth Aimbot (FOV = ON , No FOV = OFF)
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UIS = game:GetService("UserInputService")
local Camera = workspace.CurrentCamera

local LocalPlayer = Players.LocalPlayer
local Mouse = LocalPlayer:GetMouse()

-- SETTINGS
local AIM_KEY = Enum.UserInputType.MouseButton2
local FOV_RADIUS = 500
local MAX_DISTANCE = 850
local SMOOTHNESS = 1.5
local AIM_PART = "HumanoidRootPart"

local FOV_ENABLED = false -- ðŸ”µ à¹€à¸›à¸´à¸” = aimbot à¸—à¸³à¸‡à¸²à¸™

-- FOV Circle
local FOV = Drawing.new("Circle")
FOV.Color = Color3.fromRGB(255, 255, 255)
FOV.Thickness = 2
FOV.NumSides = 100
FOV.Radius = FOV_RADIUS
FOV.Filled = false
FOV.Visible = FOV_ENABLED

-- Toggle FOV (à¸„à¸§à¸šà¸„à¸¸à¸¡ aimbot à¸”à¹‰à¸§à¸¢)
UIS.InputBegan:Connect(function(input, gpe)
	if gpe then return end
	if input.UserInputType == Enum.UserInputType.MouseButton3 then
		FOV_ENABLED = not FOV_ENABLED
		FOV.Visible = FOV_ENABLED
	end
end)

-- à¸«à¸²à¹€à¸›à¹‰à¸²à¹ƒà¸™à¸§à¸‡ + à¸£à¸°à¸¢à¸°
local function GetTarget()
	local closestPart = nil
	local shortest = FOV_RADIUS

	for _, plr in ipairs(Players:GetPlayers()) do
		if plr ~= LocalPlayer and plr.Character then
			local part = plr.Character:FindFirstChild(AIM_PART)
			local hum = plr.Character:FindFirstChild("Humanoid")

			if part and hum and hum.Health > 0 then
				local distance3D = (Camera.CFrame.Position - part.Position).Magnitude
				if distance3D > MAX_DISTANCE then continue end

				local pos, onScreen = Camera:WorldToViewportPoint(part.Position)
				if onScreen then
					local dist2D = (Vector2.new(pos.X, pos.Y) -
						Vector2.new(Mouse.X, Mouse.Y)).Magnitude

					if dist2D < shortest then
						shortest = dist2D
						closestPart = part
					end
				end
			end
		end
	end

	return closestPart
end

-- Update
RunService.RenderStepped:Connect(function()
	-- à¸­à¸±à¸›à¹€à¸”à¸•à¸§à¸‡à¹€à¸‰à¸žà¸²à¸°à¸•à¸­à¸™à¹€à¸›à¸´à¸”
	if FOV_ENABLED then
		FOV.Position = Vector2.new(Mouse.X, Mouse.Y)
	end

	-- â— à¸–à¹‰à¸² FOV à¸›à¸´à¸” = aimbot à¹„à¸¡à¹ˆà¸—à¸³à¸‡à¸²à¸™
	if not FOV_ENABLED then return end

	if UIS:IsMouseButtonPressed(AIM_KEY) then
		local target = GetTarget()
		if target then
			local camCF = Camera.CFrame
			local aimCF = CFrame.new(camCF.Position, target.Position)
			Camera.CFrame = camCF:Lerp(aimCF, SMOOTHNESS)
		end
	end
end)

--// Sticky Attach Follow (Press H)
--// SERVICES
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UIS = game:GetService("UserInputService")

local LP = Players.LocalPlayer
local SPEED = 150

--// STATE
local flying = false
local attached = false
local target = nil
local selectedPlayer = nil

local flyConn, attachConn, noclipConn
local bv, bg

--// ATTACH SETTINGS
local attachDir = Vector3.new(0,1,0) -- à¸šà¸™
local attachDist = 3

--// UTILS
local function getChar(plr)
    return plr.Character or plr.CharacterAdded:Wait()
end

--// NOCLIP
local function enableNoClip()
    if noclipConn then noclipConn:Disconnect() end
    noclipConn = RunService.Stepped:Connect(function()
        local char = LP.Character
        if not char then return end
        for _,v in ipairs(char:GetDescendants()) do
            if v:IsA("BasePart") then
                v.CanCollide = false
            end
        end
    end)
end

local function disableNoClip()
    if noclipConn then noclipConn:Disconnect() noclipConn = nil end
    local char = LP.Character
    if not char then return end
    for _,v in ipairs(char:GetDescendants()) do
        if v:IsA("BasePart") then
            v.CanCollide = true
        end
    end
end

--// STOP
local function stopAll()
    flying = false
    attached = false
    target = nil

    if flyConn then flyConn:Disconnect() end
    if attachConn then attachConn:Disconnect() end
    if bv then bv:Destroy() end
    if bg then bg:Destroy() end

    local hum = getChar(LP):WaitForChild("Humanoid")
    hum.PlatformStand = false
    disableNoClip()
end

--// ATTACH
local function attachTo(plr)
    stopAll()

    local char = getChar(LP)
    local hrp = char.HumanoidRootPart
    local hum = char.Humanoid
    hum.PlatformStand = true

    attached = true
    target = plr

    attachConn = RunService.Heartbeat:Connect(function()
        if not target.Character then return end
        local tHRP = target.Character:FindFirstChild("HumanoidRootPart")
        if tHRP then
            local offset = attachDir * attachDist
            hrp.CFrame = tHRP.CFrame * CFrame.new(offset)
            hrp.Velocity = Vector3.zero
        end
    end)
end

--// FLY
local function flyTo(plr)
    stopAll()

    local char = getChar(LP)
    local hrp = char.HumanoidRootPart
    local hum = char.Humanoid
    hum.PlatformStand = true
    enableNoClip()

    bv = Instance.new("BodyVelocity", hrp)
    bv.MaxForce = Vector3.new(1e9,1e9,1e9)

    bg = Instance.new("BodyGyro", hrp)
    bg.MaxTorque = Vector3.new(1e9,1e9,1e9)
    bg.P = 1e5

    flying = true
    target = plr

    flyConn = RunService.Heartbeat:Connect(function()
        if not target.Character then return end
        local tHRP = target.Character:FindFirstChild("HumanoidRootPart")
        if not tHRP then return end

        local dir = (tHRP.Position - hrp.Position)
        bg.CFrame = CFrame.lookAt(hrp.Position, tHRP.Position)
        bv.Velocity = dir.Unit * SPEED

        if dir.Magnitude < 5 then
            attachTo(target)
        end
    end)
end

--// GUI
local gui = Instance.new("ScreenGui", LP.PlayerGui)
gui.ResetOnSpawn = false

local frame = Instance.new("Frame", gui)
frame.Size = UDim2.new(0,280,0,400)
frame.Position = UDim2.new(0,40,0.25,0)
frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
frame.Visible = false
Instance.new("UICorner", frame)

--// PLAYER LIST
local list = Instance.new("ScrollingFrame", frame)
list.Size = UDim2.new(1,-20,0,180)
list.Position = UDim2.new(0,10,0,10)
list.AutomaticCanvasSize = Enum.AutomaticSize.Y
list.ScrollingDirection = Enum.ScrollingDirection.Y
list.ScrollBarThickness = 6
list.Active = true
list.BackgroundTransparency = 1

local layout = Instance.new("UIListLayout", list)
layout.Padding = UDim.new(0,6)

--// BUTTON CREATOR
local function makeBtn(parent,text,size,pos,color)
    local b = Instance.new("TextButton", parent)
    b.Size = size
    b.Position = pos
    b.Text = text
    b.BackgroundColor3 = color
    b.TextColor3 = Color3.new(1,1,1)
    Instance.new("UICorner", b)
    return b
end

--// CONTROL BUTTONS
local flyBtn = makeBtn(frame,"à¸šà¸´à¸™à¹„à¸›à¸«à¸²",UDim2.new(1,-20,0,30),UDim2.new(0,10,0,200),Color3.fromRGB(0,170,255))
local stopBtn = makeBtn(frame,"à¸«à¸¢à¸¸à¸” / à¸›à¸¥à¹ˆà¸­à¸¢",UDim2.new(1,-20,0,26),UDim2.new(0,10,0,235),Color3.fromRGB(200,60,60))

--// ATTACH MODE LABEL
local lbl = Instance.new("TextLabel", frame)
lbl.Size = UDim2.new(1,-20,0,20)
lbl.Position = UDim2.new(0,10,0,270)
lbl.Text = "à¸•à¸³à¹à¸«à¸™à¹ˆà¸‡à¸à¸²à¸£à¹€à¸à¸²à¸°"
lbl.TextColor3 = Color3.new(1,1,1)
lbl.BackgroundTransparency = 1

--// ATTACH BUTTONS
makeBtn(frame,"à¸šà¸™",UDim2.new(0.48,0,0,26),UDim2.new(0,10,0,295),Color3.fromRGB(15,15,15))
    .MouseButton1Click:Connect(function() attachDir = Vector3.new(0,1,0) end)

makeBtn(frame,"à¸«à¸™à¹‰à¸²",UDim2.new(0.48,0,0,26),UDim2.new(0.45,10,0,295),Color3.fromRGB(15,15,15))
    .MouseButton1Click:Connect(function() attachDir = Vector3.new(0,0,-1) end)

makeBtn(frame,"à¸«à¸¥à¸±à¸‡",UDim2.new(0.48,0,0,26),UDim2.new(0,10,0,325),Color3.fromRGB(15,15,15))
    .MouseButton1Click:Connect(function() attachDir = Vector3.new(0,0,1) end)

makeBtn(frame,"à¸¥à¹ˆà¸²à¸‡",UDim2.new(0.48,0,0,26),UDim2.new(0.45,10,0,325),Color3.fromRGB(15,15,15))
    .MouseButton1Click:Connect(function() attachDir = Vector3.new(0,-1,0) end)

--// DISTANCE BUTTON
local distBtn = makeBtn(frame,"à¸£à¸°à¸¢à¸°à¹€à¸à¸²à¸° : 3",UDim2.new(1,-20,0,26),UDim2.new(0,10,0,360),Color3.fromRGB(0,170,255))
distBtn.MouseButton1Click:Connect(function()
    attachDist += 1
    if attachDist > 10 then attachDist = 1 end
    distBtn.Text = "à¸£à¸°à¸¢à¸°à¹€à¸à¸²à¸° : "..attachDist
end)

--// PLAYER LIST UPDATE
local function refreshList()
    for _,v in ipairs(list:GetChildren()) do
        if v:IsA("TextButton") then v:Destroy() end
    end

    for _,plr in ipairs(Players:GetPlayers()) do
        if plr ~= LP then
            local b = makeBtn(list,plr.Name,UDim2.new(1,-10,0,30),UDim2.new(),Color3.fromRGB(15,15,15))
            b.MouseButton1Click:Connect(function()
                selectedPlayer = plr
            end)
        end
    end
end

refreshList()
Players.PlayerAdded:Connect(refreshList)
Players.PlayerRemoving:Connect(refreshList)

--// EVENTS
flyBtn.MouseButton1Click:Connect(function()
    if selectedPlayer then flyTo(selectedPlayer) end
end)

stopBtn.MouseButton1Click:Connect(stopAll)

--// TOGGLE GUI (H)
UIS.InputBegan:Connect(function(i,gp)
    if not gp and i.KeyCode == Enum.KeyCode.H then
        frame.Visible = not frame.Visible
    end
end)

--// DRAG FRAME (à¸„à¸¥à¸´à¸à¸‹à¹‰à¸²à¸¢à¸¥à¸²à¸)
do
    local dragging = false
    local dragStart
    local startPos

    frame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragStart = input.Position
            startPos = frame.Position

            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)

    UIS.InputChanged:Connect(function(input)
        if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
            local delta = input.Position - dragStart
            frame.Position = UDim2.new(
                startPos.X.Scale,
                startPos.X.Offset + delta.X,
                startPos.Y.Scale,
                startPos.Y.Offset + delta.Y
            )
        end
    end)
end

--Flying
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local root = character:WaitForChild("HumanoidRootPart")
local camera = workspace.CurrentCamera

local flying = false
local speed = 150

local bv, bg
local move = {
	W = false,
	A = false,
	S = false,
	D = false,
	Space = false,
	Shift = false
}

-- à¹€à¸£à¸´à¹ˆà¸¡à¸šà¸´à¸™
local function startFly()
	flying = true
	humanoid.PlatformStand = true

	bv = Instance.new("BodyVelocity")
	bv.MaxForce = Vector3.new(1e9, 1e9, 1e9)
	bv.Velocity = Vector3.zero
	bv.Parent = root

	bg = Instance.new("BodyGyro")
	bg.MaxTorque = Vector3.new(1e9, 1e9, 1e9)
	bg.CFrame = root.CFrame
	bg.Parent = root
end

-- à¸«à¸¢à¸¸à¸”à¸šà¸´à¸™
local function stopFly()
	flying = false
	humanoid.PlatformStand = false

	if bv then bv:Destroy() end
	if bg then bg:Destroy() end
end

-- à¸à¸”à¸›à¸¸à¹ˆà¸¡
UserInputService.InputBegan:Connect(function(input, gp)
	if gp then return end

	if input.KeyCode == Enum.KeyCode.N then
		if flying then
			stopFly()
		else
			startFly()
		end
	end

	if input.KeyCode == Enum.KeyCode.W then move.W = true end
	if input.KeyCode == Enum.KeyCode.A then move.A = true end
	if input.KeyCode == Enum.KeyCode.S then move.S = true end
	if input.KeyCode == Enum.KeyCode.D then move.D = true end
	if input.KeyCode == Enum.KeyCode.Space then move.Space = true end
	if input.KeyCode == Enum.KeyCode.LeftShift then move.Shift = true end
end)

UserInputService.InputEnded:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.W then move.W = false end
	if input.KeyCode == Enum.KeyCode.A then move.A = false end
	if input.KeyCode == Enum.KeyCode.S then move.S = false end
	if input.KeyCode == Enum.KeyCode.D then move.D = false end
	if input.KeyCode == Enum.KeyCode.Space then move.Space = false end
	if input.KeyCode == Enum.KeyCode.LeftShift then move.Shift = false end
end)

-- à¸„à¸§à¸šà¸„à¸¸à¸¡à¸à¸²à¸£à¸šà¸´à¸™
RunService.RenderStepped:Connect(function()
	if not flying then return end

	local direction = Vector3.zero
	local camCF = camera.CFrame

	if move.W then direction += camCF.LookVector end
	if move.S then direction -= camCF.LookVector end
	if move.A then direction -= camCF.RightVector end
	if move.D then direction += camCF.RightVector end
	if move.Space then direction += camCF.UpVector end
	if move.Shift then direction -= camCF.UpVector end

	if direction.Magnitude > 0 then
		direction = direction.Unit * speed
	end

	bv.Velocity = direction
	bg.CFrame = camCF
end)

-- FlingScriptSafe
--// SERVICES
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local bav -- BodyAngularVelocity
local UIS = game:GetService("UserInputService")

local LP = Players.LocalPlayer
local FLING_POWER = 10000
local FLING_TIME = 0.1

--// STATE
local selectedTarget = nil
local oldCFrame
local flingConn
local noclipConn

--// UTILS
local function getChar(plr)
    return plr.Character or plr.CharacterAdded:Wait()
end

--// NOCLIP
local function enableNoClip()
    noclipConn = RunService.Stepped:Connect(function()
        local char = LP.Character
        if not char then return end
        for _,v in ipairs(char:GetDescendants()) do
            if v:IsA("BasePart") then
                v.CanCollide = false
            end
        end
    end)
end

local function disableNoClip()
    if noclipConn then noclipConn:Disconnect() noclipConn = nil end
    local char = LP.Character
    if not char then return end
    for _,v in ipairs(char:GetDescendants()) do
        if v:IsA("BasePart") then
            v.CanCollide = true
        end
    end
end

--// STOP
local function stopFling()
    if flingConn then flingConn:Disconnect() flingConn = nil end
    disableNoClip()

    if bav then
        bav:Destroy()
        bav = nil
    end

    local char = LP.Character
    if char and oldCFrame then
        char:PivotTo(oldCFrame)
        char.Humanoid.PlatformStand = false
    end
end

--// FLING LOGIC (PLAYER + NPC)
local function flingTarget(targetHRP)
    stopFling()

    local char = getChar(LP)
    local hrp = char:WaitForChild("HumanoidRootPart")
    local hum = char:WaitForChild("Humanoid")

    oldCFrame = hrp.CFrame
    hum.PlatformStand = true
    enableNoClip()

    -- BodyVelocity (à¹€à¸”à¸´à¸¡)
    local bv = Instance.new("BodyVelocity")
    bv.MaxForce = Vector3.new(1e9,1e9,1e9)
    bv.Parent = hrp

    -- ðŸ”µ BodyAngularVelocity (à¸«à¸¡à¸¸à¸™)
    bav = Instance.new("BodyAngularVelocity")
    bav.MaxTorque = Vector3.new(1e9, 1e9, 1e9)
    bav.AngularVelocity = Vector3.new(30, 50, 30) -- à¸›à¸£à¸±à¸šà¸„à¸§à¸²à¸¡à¹€à¸£à¹‡à¸§à¸«à¸¡à¸¸à¸™à¹„à¸”à¹‰
    bav.Parent = hrp

    flingConn = RunService.Heartbeat:Connect(function()
        if not targetHRP or not targetHRP.Parent then
            stopFling()
            bv:Destroy()
            if bav then bav:Destroy() end
            return
        end

        hrp.CFrame = targetHRP.CFrame * CFrame.new(0,0,-1)
        bv.Velocity = targetHRP.CFrame.LookVector * FLING_POWER
    end)

    task.delay(FLING_TIME, function()
        stopFling()
        bv:Destroy()
        if bav then bav:Destroy() end
    end)
end

--// GUI
local gui = Instance.new("ScreenGui", LP.PlayerGui)
gui.ResetOnSpawn = false

local frame = Instance.new("Frame", gui)
frame.Size = UDim2.new(0,300,0,420)
frame.Position = UDim2.new(0,50,0.25,0)
frame.BackgroundColor3 = Color3.fromRGB(25,25,25)
frame.Visible = false
Instance.new("UICorner", frame)

--// TITLE
local title = Instance.new("TextLabel", frame)
title.Size = UDim2.new(1,0,0,40)
title.Text = "SAFE FLING"
title.TextColor3 = Color3.fromRGB(0,170,255)
title.BackgroundTransparency = 1
title.Font = Enum.Font.GothamBold
title.TextSize = 18

--// LIST
local list = Instance.new("ScrollingFrame", frame)
list.Size = UDim2.new(1,-20,0,300)
list.Position = UDim2.new(0,10,0,45)
list.AutomaticCanvasSize = Enum.AutomaticSize.Y
list.ScrollBarThickness = 6
list.BackgroundTransparency = 1
Instance.new("UIListLayout", list)

--// BUTTON MAKER
local function makeBtn(text)
    local b = Instance.new("TextButton")
    b.Size = UDim2.new(1,-8,0,30)
    b.Text = text
    b.BackgroundColor3 = Color3.fromRGB(15,15,15)
    b.TextColor3 = Color3.new(1,1,1)
    b.Font = Enum.Font.Gotham
    b.TextSize = 14
    Instance.new("UICorner", b)
    return b
end

--// REFRESH TARGET LIST (PLAYER + NPC)
local function refreshList()
    for _,v in ipairs(list:GetChildren()) do
        if v:IsA("TextButton") then v:Destroy() end
    end

    -- Players
    for _,plr in ipairs(Players:GetPlayers()) do
        if plr ~= LP and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
            local b = makeBtn("[à¸œà¸¹à¹‰à¹€à¸¥à¹ˆà¸™] "..plr.Name)
            b.Parent = list
            b.MouseButton1Click:Connect(function()
                selectedTarget = plr.Character.HumanoidRootPart
            end)
        end
    end

    -- NPC / Monster
    for _,m in ipairs(workspace:GetDescendants()) do
        if m:IsA("Model") and not Players:GetPlayerFromCharacter(m) then
            if m:FindFirstChild("Humanoid") and m:FindFirstChild("HumanoidRootPart") then
                local b = makeBtn("[NPC] "..m.Name)
                b.Parent = list
                b.MouseButton1Click:Connect(function()
                    selectedTarget = m.HumanoidRootPart
                end)
            end
        end
    end
end

-- AUTO UPDATE
task.spawn(function()
    while true do
        refreshList()
        task.wait(2)
    end
end)

--// FLING BUTTON (KEYMO BLUE)
local flingBtn = Instance.new("TextButton", frame)
flingBtn.Size = UDim2.new(1,-20,0,36)
flingBtn.Position = UDim2.new(0,10,1,-46)
flingBtn.Text = "FLING TARGET"
flingBtn.BackgroundColor3 = Color3.fromRGB(0,170,255)
flingBtn.TextColor3 = Color3.new(1,1,1)
flingBtn.Font = Enum.Font.GothamBold
flingBtn.TextSize = 15
Instance.new("UICorner", flingBtn)

flingBtn.MouseButton1Click:Connect(function()
    if selectedTarget then
        flingTarget(selectedTarget)
    end
end)

--// TOGGLE GUI (G)
UIS.InputBegan:Connect(function(i,gp)
    if not gp and i.KeyCode == Enum.KeyCode.G then
        frame.Visible = not frame.Visible
    end
end)

--// DRAG GUI
do
    local dragging, dragStart, startPos

    frame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragStart = input.Position
            startPos = frame.Position
        end
    end)

    UIS.InputChanged:Connect(function(input)
        if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
            local delta = input.Position - dragStart
            frame.Position = UDim2.new(
                startPos.X.Scale,
                startPos.X.Offset + delta.X,
                startPos.Y.Scale,
                startPos.Y.Offset + delta.Y
            )
        end
    end)

    UIS.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
        end
    end)
end

















